    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="25" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.1" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.asus.weclass.SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.asus.weclass.MainActivity" />
        <activity android:name="com.example.asus.weclass.LoginActivity" />
        <activity android:name="com.example.asus.weclass.PassActivity" />
        <activity android:name="com.example.asus.weclass.AppActivity" />
        <activity android:name="com.example.asus.weclass.AppListActivity" />
        <activity android:name="com.example.asus.weclass.AppDetActivity" />
        <activity android:name="com.example.asus.weclass.ClassListActivity" />
        <activity android:name="com.example.asus.weclass.ReLiActivity" />
        <activity android:name="com.example.asus.weclass.ReqDetActivity" />
        <activity android:name="com.example.asus.weclass.RoomDetActivity" />

        <provider
            android:name="com.android.tools.fd.runtime.InstantRunContentProvider"
            android:authorities="com.example.asus.weclass.com.android.tools.fd.runtime.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>